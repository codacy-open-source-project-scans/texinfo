#: info/info.c:1154
msgid ""
"  info --subnodes -o out.txt emacs\n"
"                               dump entire emacs manual to out.txt"
msgstr ""

#: info/info.c:1157
msgid "  info -f ./foo.info           show file ./foo.info, not searching dir"
msgstr ""

